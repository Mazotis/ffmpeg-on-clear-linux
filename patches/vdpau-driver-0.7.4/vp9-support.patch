+// Dumps matrix[N][M] = N rows x M columns (uint16_t)
+static void
+dump_matrix_NxM_short(const char *label, const uint16_t *matrix, int N, int M, int L)
+{
+    int i, j, n = 0;
+
+    TRACE(".%s = {\n", label);
+    INDENT(1);
+    for (j = 0; j < N; j++) {
+        for (i = 0; i < M; i++, n++) {
+            if (n >= L)
+                break;
+            if (i > 0)
+                TRACE(", ");
+            TRACE("0x%02x", matrix[n]);
+        }
+        if (j < (N - 1))
+            TRACE(",");
+        TRACE("\n");
+        if (n >= L)
+            break;
+    }
+    INDENT(-1);
+    TRACE("}\n");
+}
